{
  "name": "go-storybook",
  "version": "0.1.17",
  "description": "react storybook",
  "keywords": [
    "react",
    "storybook",
    "typescript",
    "component",
    "presentor"
  ],
  "homepage": "https://imki123.github.io/go-storybook/",
  "repository": "imki123/go-storybook",
  "author": "imki123 <popping2606@gmail.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "./browser/specific/main.js",
  "auto": {
    "plugins": [
      "npm"
    ],
    "onlyPublishWithReleaseLabel": true
  },
  "scripts": {
    "sb": "yarn storybook",
    "storybook": "start-storybook -p 6006 -s public",
    "build": "build-storybook -s public",
    "deploy": "yarn build && yarn gh-pages -d storybook-static",
    "build-npm": "rm -rf dist && mkdir dist && tsc",
    "auto": "auto changelog",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "gh-pages": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-backgrounds": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-measure": "^6.5.16",
    "@storybook/addon-outline": "^6.5.16",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/client-api": "^6.5.16",
    "@storybook/client-logger": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-react": "^3.1.0",
    "auto": "^10.37.6",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint-config-react-app": "^6.0.0",
    "prop-types": "^15.8.1",
    "vite": "^4.1.1",
    "webpack": "^5.75.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.4.1"
}
